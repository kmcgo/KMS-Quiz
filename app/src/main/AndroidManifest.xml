<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kmsquiz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AddQuestionActivity"></activity>
        <activity android:name=".AddQuizActivity" />
        <activity
            android:name=".ManageUserActivity"
            android:label="@string/title_activity_manage_user"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ManageQuizActivity"
            android:label="@string/title_activity_manage_quiz"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".QuizActivity"
            android:label="@string/title_activity_quiz" />
        <activity
            android:name=".UserActivity"
            android:label="@string/title_activity_user" />
        <activity
            android:name=".AdminActivity"
            android:label="@string/title_activity_admin" />
        <activity
            android:name=".StartingScreenActivity"
            android:label="@string/title_activity_starting_screen" />
        <activity
            android:name=".ui.login.LoginActivity"
            android:label="KMS Quiz">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>